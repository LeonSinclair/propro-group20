@model Tuple< List<Course>, List<User> >

@{
    ViewData["Title"] = "Lecturer View";
}
<div id="body_container">

    <div class="section" id="module">
        <h4>Modules</h4>

        <!-- search box -->
        <input type="text" placeholder="Type module and press enter" class="search">
        <input type="button" class="button" value="Apply Filter">
        <!-- /search box -->
        <div class="container">
            @foreach (var item in Model.Item1)
            {


            <div class="row">
                <div class="col-sm">
                    <a href="">
                        <div class="object_box">
                            @Html.DisplayFor(modelItem => item.Code)
                            @Html.DisplayFor(modelItem => item.Name)
                        </div>
                    </a>
                </div>
            </div>

            }
        </div>
        <div class="view_all">
            <a href="#">View all modules</a>
        </div>

    </div>

    <div class="section" id="demonstrator">
        <h4>Demonstrators</h4>
        <!-- search box -->
        <input type="text" placeholder="Type module and press enter" class="search">
        <input type="button" class="button" value="Apply Filter">
        <!-- /search box -->
        <div class="container">
            @foreach (var item in Model.Item2.Take(5))
            {
            <div class="row">
                <div class="col-sm">
                    <a href="">
                        <div class="object_box">
                            @Html.DisplayFor(modelItem => item.FirstName)
                            @Html.DisplayFor(modelItem => item.LastName)
                            @Html.DisplayFor(modelItem => item.Skills)
                        </div>
                        <div class="propose">
                            <form action="employee.action" method="post">
                                <a asp-action="ProposeDemonstrator" asp-route-id="@item.Id">Propose Module</a> |
                                <a asp-action="DemonstratorDetails" asp-route-id="@item.Id">Details</a>
                            </form>

                        </div>
                    </a>
                </div>
            </div>

            }
        </div>
            <div class="view_all">
                <a href="ModuleDemonstrators">View all demonstrators</a>
            </div>

        </div>
</div>

<script language="javascript">


    function DoPost() {
        $.post("ModuleDemonstrators", { moduleId: "2041" });  //Your values here..
    }

</script>